{
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "**/*.avif",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/avif"
        }
      ]
    }
  ],
  "redirects": [{ "source": "/editor", "destination": "/" }]
}
